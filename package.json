{
  "name": "angular-es6-starter",
  "version": "0.0.1",
  "description": "Simple starter kit for angular and es6",
  "main": "./bin/server.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jordond/angular-es6-starter.git"
  },
  "author": "Jordon de Hoog",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jordond/angular-es6-starter/issues"
  },
  "homepage": "https://github.com/jordond/angular-es6-starter#readme",
  "scripts": {
    "test": "better-npm-run test",
    "test-watch": "npm test -- --watch",
    "lint": "eslint -c .eslintrc src",
    "start": "better-npm-run start:prod",
    "start-dev": "better-npm-run start:dev",
    "build": "npm run lint && better-npm-run build:prod",
    "build-watch": "npm run lint && better-npm-run build:prod:watch",
    "build-dev": "better-npm-run build:dev",
    "dev": "concurrently --kill-others \"npm run build-dev\" \"npm run start-dev\"",
    "dev-test": "concurrently --kill-others \"npm run test-watch\" \"npm run dev\"",
    "dev-prod": "concurrently --kill-others \"npm run build-watch\" \"npm run start\"",
    "hook-install": "precommit install",
    "hook-remove": "precommit remove",
    "upgrade": "git pull && npm install"
  },
  "precommit": [
    "npm run lint"
  ],
  "betterScripts": {
    "test": {
      "command": "mocha-webpack --colors --require source-map-support/register --require src/client/testHelper.js --webpack-config config.webpack.js \"src/client/**/*.test.js\"",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "start:prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "./src",
        "PORT": 8080
      }
    },
    "start:dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "./src",
        "PORT": 9000
      }
    },
    "build:prod": {
      "command": "webpack --verbose --colors --display-error-details --config config.webpack.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:prod:watch": {
      "command": "webpack --watch --debug --verbose --colors --display-error-details --config config.webpack.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:dev": {
      "command": "webpack --watch --debug --colors --display-error-details --config config.webpack.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 9000
      }
    }
  },
  "dependencies": {
    "express": "^4.13.4",
    "morgan": "^1.7.0",
    "pretty-error": "^2.0.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-aria": "^1.5.0",
    "angular-material": "^1.0.6",
    "angular-messages": "^1.5.0",
    "angular-mocks": "^1.5.3",
    "angular-moment": "^0.10.3",
    "angular-ui-router": "^0.2.18",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-runtime": "^6.6.1",
    "better-npm-run": "0.0.8",
    "browser-sync": "^2.11.1",
    "browser-sync-webpack-plugin": "^1.0.1",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.8",
    "compression": "^1.6.1",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.0.3",
    "eslint-plugin-react": "^4.2.0",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.5.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.9.0",
    "ignore-loader": "^0.1.1",
    "jsdom": "^8.3.1",
    "mocha": "^2.4.5",
    "mocha-webpack": "^0.3.0",
    "ng-annotate": "^1.2.1",
    "ng-annotate-loader": "^0.1.0",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^3.4.2",
    "normalize.css": "^3.0.3",
    "precommit": "^1.2.0",
    "sass-loader": "^3.1.2",
    "sass-material-colors": "0.0.5",
    "skeleton-css": "^2.0.4",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15",
    "webpack-node-externals": "^1.0.0"
  }
}
